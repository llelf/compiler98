#!/bin/sh
case $1 in
  base)	echo Data Debug Control Control/Monad System System/IO System/Console
	echo Text Text/ParserCombinators Text/ParserCombinators/Parsec
	echo Text/PrettyPrint Text/Html Text/Show
	echo Foreign Foreign/Marshal Foreign/C NHC
	;;
  QuickCheck)
	echo Debug Debug/QuickCheck
	;;
  Japi) echo Graphics Graphics/UI Graphics/UI/Japi
	;;
  *) ;;
esac
